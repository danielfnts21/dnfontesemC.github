#include <stdio.h>

int main() {
    int numeros[10];
    int divisivelPor5E3 = 0; // Contador para números divisíveis por 5 e 3 ao mesmo tempo

  
    printf("Digite dez numeros inteiros:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &numeros[i]);
    }

 
    for (int i = 0; i < 10; i++) {
        if (numeros[i] % 5 == 0 && numeros[i] % 3 == 0) {
            divisivelPor5E3++;
        }
    }

    
    printf("Quantidade de numeros divisiveis por 5 e por 3 ao mesmo tempo: %d\n", divisivelPor5E3);

    return 0;
}
